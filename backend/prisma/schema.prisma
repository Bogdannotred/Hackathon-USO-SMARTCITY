generator client {
  provider = "prisma-client-js"
  output   = "src/generated/client" //Specify the output directory
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int     @id @default(autoincrement())
  email    String  @unique
  name     String?
  password String
}
